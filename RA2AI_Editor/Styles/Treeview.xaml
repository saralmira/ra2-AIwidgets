                        <DataTrigger Binding="{Binding HighLight}" Value="True">
                            <Setter Property="Background" Value="{StaticResource backgroundhl}" TargetName="itemBackground"/>
                            <Setter Property="BorderBrush" Value="#7DA2CE" TargetName="itemBackground"/>
                            <Setter Property="Effect" TargetName="itemBackground">
                                <Setter.Value>
                                    <DropShadowEffect BlurRadius="5" Opacity="1" Color="#7DA2CE" ShadowDepth="0"/>
                                </Setter.Value>
                            </Setter>
                        </DataTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

